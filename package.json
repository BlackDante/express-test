{
  "name": "express-server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "start:prod": "pm2 start ./bin/www",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test mocha -R spec tests/unit-test/test.spec.js",
    "test:integration": "NODE_ENV=test mocha -R spec tests/integration/test.spec.js"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "helmet": "^2.1.2",
    "http-codes": "^1.0.0",
    "jade": "~1.11.0",
    "mongoose": "^4.5.8",
    "morgan": "~1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pug": "^2.0.0-beta4",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-plugin-node": "^2.0.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.0"
  }
}
